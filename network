[connection]
id=my-network
uuid=NUEVO_UUID
type=802-11-wireless

[802-11-wireless]
mode=infrastructure
ssid=NOMRE_DE_TU_WIFI
# Uncomment below if your SSID is not broadcasted
#hidden=true

[802-11-wireless-security]
auth-alg=open
key-mgmt=wpa-psk
psk=CLAVE_DE_TU_WIFI

[ipv4]
method=auto

[ipv6]
addr-gen-mode=stable-privacy
method=auto
